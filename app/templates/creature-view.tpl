<div class="background background-<%= element %>">
	<div class="row card-item">
		<div class="col-md-12">
			<h3 class="pull-left"><%= name %></h3>
			<h3 class="pull-right"><%= element %></h3>
		</div>
	</div>
	<div class="row crop">
		<div class="col-md-12">
					<img class="img-rounded" src='<%= img %>' />
				</div>
	</div>
	<div class="row card-item">
		<div class="col-md-12">
			<b>Offensive skill</b>
			<h4><%= attack %></h4>
		</div>
	</div>
	<div class="row card-item">
		<div class="col-md-12">
			<b>Defensive skill</b>
			<h4><%= defense %></h4>
		</div>
	</div>
</div>